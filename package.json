{
  "name": "astro-honobun",
  "type": "module",
  "scripts": {
    "editorconfig": "editorconfig-checker -exclude \"**/node_modules/**\"",
    "dev": "bunx --bun astro dev --verbose",
    "start": "bun wrangler types && bun astro check --verbose && bunx --bun astro dev --compatibility-flags",
    "build": "bun wrangler types && bun astro check --verbose && bunx --bun astro build --verbose",
    "preview": "bunx --bun astro build && bun wrangler pages dev ./dist",
    "deploy": "bunx --bun astro build && bun wrangler pages deploy ./dist",
    "browserslist:update": "bunx update-browserslist-db@latest",
    "cf-typegen": "wrangler types",
    "format": "biome format . --write",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --apply",
    "lint:fix:unsafe": "biome lint . --apply-unsafe",
    "biomes": "bunx @biomejs/biome check --write .",
    "biomes:fix": "bunx @biomejs/biome check --fix --unsafe .",
    "tailwind:cli": "bunx @tailwindcss/cli@next -i ./src/styles/tailwind.css -o ./src/styles/dist/tailwind.css",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "0.9.2",
    "@astrojs/cloudflare": "^11.0.4",
    "@astrojs/markdoc": "^0.11.3",
    "@astrojs/node": "^8.3.2",
    "@astrojs/partytown": "^2.1.1",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/starlight": "0.25.5",
    "@astrojs/ts-plugin": "^1.9.2",
    "@keystatic/astro": "^5.0.0",
    "@keystatic/core": "^0.5.32",
    "@nanostores/persistent": "^0.10.2",
    "@nanostores/react": "^0.7.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@tailwindcss/cli": "^4.0.0-alpha.19",
    "@tailwindcss/postcss": "^4.0.0-alpha.19",
    "@tailwindcss/vite": "^4.0.0-alpha.19",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.13.3",
    "astro-auto-import": "^0.4.2",
    "astro-expressive-code": "^0.35.6",
    "astro-robots-txt": "^1.0.0",
    "caniuse-lite": "^1.0.30001651",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "detect-browser-es": "^0.1.0",
    "editorconfig-checker": "^5.1.8",
    "framer-motion": "^11.3.24",
    "fuse.js": "^7.0.0",
    "lucide-react": "^0.427.0",
    "mdast-util-to-string": "^4.0.0",
    "motion": "^10.18.0",
    "nanostores": "^0.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "sharp": "^0.32.6",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^4.0.0-alpha.19",
    "typescript": "^5.5.4",
    "undici-types": "^6.19.6",
    "unplugin-icons": "^0.19.2"
  },
  "engines": {
    "node": "20.15.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@cloudflare/workers-types": "^4.20240806.0",
    "@types/bun": "latest",
    "@types/html": "^1.0.4",
    "@types/web": "^0.0.154",
    "unplugin-auto-import": "^0.18.2",
    "wrangler": "^3.71.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "overrides": {
    "astro-expressive-code": "latest"
  },
  "version": "1.0.0",
  "description": "To install dependencies:",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "trustedDependencies": ["@biomejs/biome", "esbuild", "sharp", "workerd"]
}
